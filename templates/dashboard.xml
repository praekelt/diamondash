<html xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
  <head>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type" />
    <link rel="stylesheet" type="text/css" href="/static/dashboard.css" />
    <script src="/static/jquery.min.js" type="text/javascript"></script>
    <script src="/static/d3.v2.min.js" type="text/javascript"></script>
    <script src="/static/rickshaw.min.js" type="text/javascript"></script>
  </head>
  <body>
    <div id="dashboard-container">
      <div t:render="widget">
        <t:attr name="class">
          <t:slot name="widget_class_slot" />
        </t:attr>
        <t:attr name="id">
          <t:slot name="widget_id_slot" />
        </t:attr>
        <span class="widget-name">
          <t:slot name="widget_name_slot"/>
        </span>
      </div>
      <script src="/static/diamondash.js" type="text/javascript"></script>
    </div>
  </body>
</html>
