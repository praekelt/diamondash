<html xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
  <head>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type" />
	<link rel="stylesheet" type="text/css" href="/static/css/style.css" />
	<link rel="stylesheet" type="text/css" href="/static/css/dashboard.css" />
	<link rel="stylesheet" type="text/css" href="/static/lib/rickshaw/rickshaw.min.css" />
	<script src="/static/lib/jquery/jquery.min.js" type="text/javascript"></script>
	<script src="/static/lib/d3/d3.v2.min.js" type="text/javascript"></script>
	<script src="/static/lib/rickshaw/rickshaw.min.js" type="text/javascript"></script>

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#">diamondash</a>
          <div class="nav-collapse">
            <ul class="nav">
              <li class="active"><a href="#">dashboard name</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <div class="container" id="dashboard-container">
		<div class='row'>
			<div t:render="widget_renderer">
				<t:attr name="class">
					<t:slot name="widget_class_slot" />
				</t:attr>
				<t:attr name="id">
					<t:slot name="widget_id_slot" />
				</t:attr>
				<h4 class="widget-title">
					<t:slot name="widget_title_slot"/>
				</h4>
				<t:slot name="widget_element_slot" />
			</div>
		</div>
	  <script t:render="config_script"><t:slot name="client_config"/></script>
	  <script src="/static/js/Widget.js" type="text/javascript"></script>
	  <script src="/static/js/GraphWidget.js" type="text/javascript"></script>
	  <script src="/static/js/LValueWidget.js" type="text/javascript"></script>
	  <script src="/static/js/diamondash.js" type="text/javascript"></script>
    </div>
  </body>
</html>
