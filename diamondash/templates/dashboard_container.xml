<div xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1" class="container" id="dashboard-container">
	<t:transparent t:render="widget_row_renderer"/>
	<script t:render="stylesheets_renderer">
		<t:attr name="href">
			<t:slot name="stylesheet_href_slot"/>
		</t:attr>
	</script>
	<script t:render="javascripts_renderer">
		<t:attr name="src">
			<t:slot name="script_src_slot"/>
		</t:attr>
	</script>
	<script t:render="init_script_renderer">
        var controller = DashboardController.fromConfig(
            <t:slot name="client_config_slot"/>);
        controller.start();
	</script>
</div>
