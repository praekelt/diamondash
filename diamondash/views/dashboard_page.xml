<html xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1">
  <head>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type" />
    <link href="css/dashboard.css" rel="stylesheet" type="text/css"/>
    <script src="js/diamondash.js"></script>

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <t:transparent t:render="brand_renderer" />

          <a t:render="brand_renderer" class="brand">
            <t:attr name="href">
              <t:slot name="brand_href_slot" />
            </t:attr>
            diamondash
          </a>

          <div class="nav-collapse">
            <ul class="nav">
              <li class="active">
                <a href="#">
                  <t:transparent t:render="dashboard_name_renderer" />
                </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <t:transparent t:render="dashboard_container_renderer" />
  </body>
</html>
